    "def abs1(list1):\n",
    "    list1 = list1[:] # copy it\n",
    "    for i in range(len(list1)):\n",
    "        if list1[i] < 0:\n",
    "            list1[i] = -list1[i]\n",
    "    return list1\n",
    "\n",
    "abs1([-1, -3, 5, -4, 8])\n"
   ]
